{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "@jspaste/frontend",
	"private": true,
	"license": "EUPL-1.2",
	"type": "module",
	"scripts": {
		"build": "bun next build",
		"fix": "bun run fix:biome && bun run fix:package",
		"fix:biome": "bun --bun biome check --apply .",
		"fix:package": "bun --bun sort-package-json --quiet",
		"lint": "bun run lint:biome && bun run lint:tsc",
		"lint:biome": "bun --bun biome lint .",
		"lint:tsc": "bun --bun tsc --noEmit",
		"start": "bun run build && bun next start",
		"start:dev": "bun next dev"
	},
	"dependencies": {
		"@chakra-ui/icons": "~2.1.1",
		"@chakra-ui/next-js": "~2.2.0",
		"@chakra-ui/react": "~2.8.2",
		"@chakra-ui/system": "~2.6.2",
		"@monaco-editor/react": "~4.6.0",
		"@types/bun": "~1.0.12",
		"@types/react": "~18.2.74",
		"@types/react-dom": "~18.2.24",
		"env-var": "~7.4.1",
		"highlight.js": "~11.9.0",
		"million": "~3.0.6",
		"next": "~14.1.4",
		"react": "~18.2.0",
		"react-dom": "~18.2.0",
		"react-icons": "~5.0.1",
		"sharp": "~0.33.3",
		"typescript": "~5.4.4",
		"zustand": "~4.5.2"
	},
	"devDependencies": {
		"@biomejs/biome": "~1.6.4",
		"lefthook": "~1.6.8",
		"sort-package-json": "~2.10.0"
	},
	"trustedDependencies": ["@biomejs/biome", "lefthook"]
}
