{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "@jspaste/frontend",
	"private": true,
	"license": "EUPL-1.2",
	"type": "module",
	"scripts": {
		"build": "bun run build:standalone",
		"build:standalone": "bun run build:vite && bun run postbuild:3step && bun run postbuild:compress",
		"build:vite": "bun vite build",
		"fix": "bun run fix:biome && bun run fix:package",
		"fix:biome": "bun biome check --write",
		"fix:package": "bun sort-package-json --quiet",
		"lint": "bun run lint:biome && bun run lint:tsc",
		"lint:biome": "bun biome lint",
		"lint:tsc": "bun tsc --noEmit",
		"postbuild:3step": "bun run ./scripts/3step.ts",
		"postbuild:compress": "bun run ./scripts/compress.ts",
		"start": "bun run start:server",
		"start:dev": "bun vite",
		"start:force": "bun run build && bun run start:server",
		"start:server": "NODE_ENV=production bun run --cwd=./dist/ ./server/index.js"
	},
	"dependencies": {
		"@swc/core": "~1.7.11",
		"@types/bun": "~1.1.6",
		"@types/react": "~18.3.3",
		"@types/react-dom": "~18.3.0",
		"@uiw/codemirror-extensions-hyper-link": "~4.23.0",
		"@uiw/codemirror-extensions-langs": "~4.23.0",
		"@uiw/codemirror-theme-vscode": "~4.23.0",
		"@uiw/codemirror-themes": "~4.23.0",
		"@uiw/react-codemirror": "~4.23.0",
		"@vitejs/plugin-react": "~4.3.1",
		"autoprefixer": "~10.4.20",
		"chalk": "~5.3.0",
		"daisyui": "~4.12.10",
		"loglevel": "~1.9.1",
		"mime": "~4.0.4",
		"postcss": "~8.4.41",
		"react": "~18.3.1",
		"react-dom": "~18.3.1",
		"react-icons": "~5.3.0",
		"tailwindcss": "~3.4.10",
		"typescript": "~5.5.4",
		"vike": "~0.4.184",
		"vike-node": "~0.1.14",
		"vike-react": "~0.5.3",
		"vite": "~5.4.1",
		"zustand": "~4.5.5"
	},
	"devDependencies": {
		"@biomejs/biome": "~1.8.3",
		"lefthook": "~1.7.13",
		"sort-package-json": "~2.10.0"
	},
	"trustedDependencies": [
		"@biomejs/biome",
		"lefthook"
	]
}
