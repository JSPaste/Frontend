{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "@jspaste/frontend",
	"private": true,
	"license": "EUPL-1.2",
	"type": "module",
	"scripts": {
		"build": "bun run build:server",
		"build:server": "rm -rf ./dist/ && bun --bun vite build && bun run script:standalone && bun run script:minify && bun run script:compress",
		"dev": "bun run start:dev",
		"fix": "bun run fix:biome && bun run fix:package",
		"fix:biome": "bun biome check --write",
		"fix:package": "bun sort-package-json --quiet",
		"lint": "bun run lint:biome && bun run lint:tsc",
		"lint:biome": "bun biome lint",
		"lint:tsc": "bun tsc --noEmit",
		"script:compress": "bun run ./scripts/compress.ts",
		"script:minify": "bun run ./scripts/minify.ts",
		"script:standalone": "bun run ./scripts/standalone.ts",
		"start": "bun run start:server",
		"start:dev": "bun --bun vite",
		"start:rebuild": "bun run build:server && bun run start:server",
		"start:server": "NODE_ENV=production bun run --cwd=./dist/ ./server/index.js"
	},
	"dependencies": {
		"@swc/core": "~1.7.24",
		"@types/bun": "~1.1.8",
		"@types/react": "~18.3.5",
		"@types/react-dom": "~18.3.0",
		"@uiw/codemirror-extensions-hyper-link": "~4.23.1",
		"@uiw/codemirror-extensions-langs": "~4.23.1",
		"@uiw/codemirror-theme-vscode": "~4.23.1",
		"@uiw/codemirror-themes": "~4.23.1",
		"@uiw/react-codemirror": "~4.23.1",
		"@vercel/nft": "~0.27.4",
		"@vitejs/plugin-react": "~4.3.1",
		"autoprefixer": "~10.4.20",
		"chalk": "~5.3.0",
		"daisyui": "~4.12.10",
		"loglevel": "~1.9.2",
		"postcss": "~8.4.45",
		"react": "~18.3.1",
		"react-dom": "~18.3.1",
		"react-icons": "~5.3.0",
		"tailwindcss": "~3.4.10",
		"typescript": "~5.5.4",
		"vike": "~0.4.195",
		"vike-react": "~0.5.5",
		"vite": "~5.4.3",
		"vite-plugin-pwa": "~0.20.5",
		"zustand": "~4.5.5"
	},
	"devDependencies": {
		"@biomejs/biome": "~1.8.3",
		"lefthook": "~1.7.15",
		"sort-package-json": "~2.10.1"
	},
	"trustedDependencies": [
		"@biomejs/biome",
		"lefthook"
	]
}
