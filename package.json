{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "jspaste-frontend",
	"private": true,
	"license": "EUPL-1.2-or-later",
	"type": "module",
	"scripts": {
		"build": "bunx next build",
		"dev": "bunx next dev",
		"fix": "bun run prettier && bun run lint",
		"lint": "bun run lint:tsc",
		"lint:tsc": "bunx tsc --noEmit",
		"prettier": "bunx prettier . --write",
		"prod": "bun run build && bun run start",
		"start": "bunx --bun next start"
	},
	"dependencies": {
		"@chakra-ui/icons": "^2.1.1",
		"@chakra-ui/next-js": "^2.2.0",
		"@chakra-ui/react": "^2.8.2",
		"@chakra-ui/system": "^2.6.2",
		"@monaco-editor/react": "^4.6.0",
		"highlight.js": "^11.9.0",
		"jspaste": "~10.1.1",
		"million": "^2.6.4",
		"next": "^14.0.4",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-icons": "^5.0.1",
		"sharp": "^0.33.2",
		"zustand": "^4.4.7"
	},
	"devDependencies": {
		"@types/bun": "latest",
		"@types/react": "^18.2.48",
		"@types/react-dom": "^18.2.18",
		"prettier": "^3.2.4",
		"prettier-plugin-jsdoc": "^1.3.0",
		"prettier-plugin-packagejson": "^2.4.9",
		"typescript": "~5.3.3"
	}
}
