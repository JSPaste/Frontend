{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "jspaste-frontend",
	"private": true,
	"license": "EUPL-1.2-or-later",
	"type": "module",
	"scripts": {
		"build": "pnpm exec next build",
		"dev": "pnpm exec next dev",
		"fix": "pnpm run prettier && pnpm run lint",
		"lint": "pnpm run lint:eslint && pnpm run lint:next && pnpm run lint:tsc",
		"lint:eslint": "pnpm exec eslint . --ext .ts,.tsx",
		"lint:next": "pnpm exec next lint",
		"lint:tsc": "pnpm exec tsc --noEmit",
		"prettier": "pnpm exec prettier . --write",
		"prod": "pnpm run build && pnpm run start",
		"start": "pnpm exec next start"
	},
	"dependencies": {
		"@chakra-ui/icons": "^2.1.1",
		"@chakra-ui/next-js": "^2.2.0",
		"@chakra-ui/react": "^2.8.2",
		"@chakra-ui/system": "^2.6.2",
		"@monaco-editor/react": "^4.6.0",
		"highlight.js": "^11.9.0",
		"jspaste": "^10.1.1",
		"million": "^2.6.4",
		"next": "^14.0.4",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-icons": "^5.0.1",
		"sharp": "^0.33.2",
		"zustand": "^4.4.7"
	},
	"devDependencies": {
		"@types/node": "^20.11.0",
		"@types/react": "^18.2.47",
		"@types/react-dom": "^18.2.18",
		"eslint": "^8.56.0",
		"eslint-config-next": "^14.0.4",
		"eslint-config-prettier": "^9.1.0",
		"prettier": "^3.1.1",
		"prettier-plugin-jsdoc": "^1.3.0",
		"prettier-plugin-packagejson": "^2.4.9",
		"typescript": "~5.3.3"
	},
	"packageManager": "pnpm@8.14.0"
}
