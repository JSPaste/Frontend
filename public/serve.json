{
	"rewrites": [{ "source": "/**", "destination": "/document.html" }],
	"redirects": [
		{
			"source": "/github",
			"destination": "https://github.com/jspaste",
			"permanent": true
		},
		{
			"source": "/favicon.ico",
			"destination": "/media/jspaste.bg.rounded.svg",
			"permanent": false
		},
		{
			"source": "/:key/raw",
			"destination": "/documents/:key/raw",
			"permanent": true
		},
		{
			"source": "/:key/r",
			"destination": "/documents/:key/raw",
			"permanent": true
		}
	]
}
