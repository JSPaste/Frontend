---
import { EditorScreen } from '@/layouts/EditorScreen';
import RootLayout from '@/layouts/RootLayout.astro';

type Props = {
	documentName?: string;
	enableEdit?: boolean;
};

const { documentName, enableEdit = false } = Astro.props as Props;
---

<RootLayout>
    <EditorScreen client:only="preact" documentName={documentName} enableEdit={enableEdit}/>
</RootLayout>